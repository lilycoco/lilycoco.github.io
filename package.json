{
  "name": "my_page",
  "version": "0.1.0",
  "description": "lilycoco's homepage",
  "repository": "https://github.com/lilycoco/lilycoco.github.io",
  "private": true,
  "scripts": {
    "dev_server": "node server.js",
    "dev": "next",
    "start": "cross-env NODE_ENV=production node server.js",
    "build": "next build && next export",
    "export": "NODE_ENV=production node scripts/buildBlogJson.js && next build",
    "deploy": "npm run build && sh scripts/deploy.sh",
    "eslint": "eslint --fix --ext .js,.ts,.tsx ./src"
  },
  "dependencies": {
    "@mdx-js/loader": "^1.6.22",
    "@next/mdx": "^9.5.5",
    "@zeit/next-typescript": "^1.1.1",
    "bootstrap": "^4.6.0",
    "css-loader": "^1.0.1",
    "express": "^4.17.1",
    "front-matter": "^3.2.1",
    "isomorphic-unfetch": "^3.1.0",
    "markdown-it": "^9.1.0",
    "markdown-it-highlightjs": "^3.4.0",
    "moment": "^2.29.1",
    "next": "^10.0.7",
    "react": "^16.14.0",
    "react-bootstrap": "^1.5.0",
    "react-dom": "^16.14.0",
    "react-scripts": "^3.4.4",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@types/jest": "24.0.15",
    "@types/markdown-it": "0.0.8",
    "@types/next": "^8.0.7",
    "@types/node": "12.0.10",
    "@types/react": "16.8.22",
    "@types/react-dom": "16.8.4",
    "@types/styled-components": "^4.4.3",
    "@types/styled-jsx": "^2.2.8",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^4.15.3-alpha.12",
    "cross-env": "^5.2.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^2.5.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.19.1",
    "typescript": "^3.9.9"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{css,json,yml,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "_moduleAliases": {
    "~": "src"
  }
}
